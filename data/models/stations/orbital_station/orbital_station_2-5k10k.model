version 2

material "facade" {
	shader "multi"
	texture diffuse  "facades_diff.dds"
	texture glow     "facades_diff.dds"
	specular 0.1 0.1 0.1
	unlit
}

material "floor" {
	shader "multi"
	texture diffuse  "floors_diff.dds"
	texture glow     "floors_diff.dds"
	specular 0.1 0.1 0.1
	unlit
}

material "tunnels" {
	shader "multi"
	texture diffuse  "spoke_n_misc_diff.dds"
	texture glow     "spoke_n_misc_diff.dds"
	specular 0.1 0.1 0.1
	unlit
}

material "facade_window" {
	shader "multi"
	texture diffuse  "facades_diff.dds"
	texture glow     "facades_diff.dds"
	specular 0.6 0.6 0.6
	opacity 13
}

material "ring" {
	shader "multi"
	texture diffuse  "ring_diff.dds"
	texture specular "ring_diff_s.dds"
	texture glow     "ring_glow.dds"
}

material "ring_glass" {
	shader "multi"
	texture diffuse  "ring_diff.dds"
	specular 0.7 0.7 0.7
	opacity 20
}

material "strut" {
	shader "multi"
	texture diffuse  "struts_diff.dds"
	specular 0.1 0.1 0.1
}

material "spokes" {
	shader "multi"
	texture diffuse  "spoke_n_misc_diff.dds"
	specular 0.1 0.1 0.1
	use_patterns
}

lod 100 {
	mesh "station_hub_A.dae"
	mesh "station_docking_A.dae"
	mesh "station_spokes_2-5k.dae"
	mesh "station_spokes_2-5k10k.dae"
	mesh "station_ring_2-5k.dae"
	mesh "station_ring_2-10k.dae"
}

collision "station_hub_A_coll.dae"
collision "station_ring_2-5k10k_coll.dae"

bound capsule interior "tag_interior0" "tag_interior1" 70
bound capsule interior "tag_interior1" "tag_interior2" 110
bound capsule interior "tag_interior2" "tag_interior3" 70
