version 2

material "fuselage-material" {
	shader "multi"
	diffuse 1.0 1.0 1.0
	specular 0.8 0.8 0.8
	use_patterns
	texture normal   "skipjack_norm.png"
	texture diffuse  "skipjack_diff.dds"
	texture specular "skipjack_spec.dds"
	texture glow     "skipjack_glow.dds"
}

lod 300 {
	mesh "skipjack_hi.dae"
}

lod 100 {
	mesh "skipjack_med.dae"
}

lod 50 {
	mesh "skipjack_low.dae"
}

anim "gear_down" 1 121

collision "skipjack_collision.dae"
